- content_for :page_title, "Your Repos"

= render 'shared/header'

:javascript
  Settings = {
    stripePublishableKey: '#{ENV['STRIPE_PUBLISHABLE_KEY']}',
    userEmailAddress: '#{current_user.email_address}'
  };

.dashboard
  %aside
    %section.single-org
      %header.current-org
        .title
          .icon
          %h2 thoughtbot
        .menu-toggle
      %nav
        %ul
          %li
            .icon
            = link_to "Repositories", "#"
          %li
            .icon
            = link_to "Ruby Styles", "#"
          %li
            .icon
            = link_to "Javascript Styles", "#"

    %section.all-orgs
  %section.repo-listing{ 'repo-list' => '' }
  = render 'preload_template', template: 'repo_list'
